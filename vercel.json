{
    "functions": {
      "pages/**/*.html": {
        "memory": 128
      }
    },
    "headers": [
      {
        "source": "/images/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }